	}
/* HOVER EFFECTS END */


// From Radio filter independent css defined by Umesh (BEGIN)
a.ibm_cci-gototop {
	visibility: hidden;
	position: fixed;
	right: 20px;
	bottom: 0px;
	color: #000;
	font: normal 0.9em 'helveticaneuelight',HelveticaNeue,Helvetica,Arial,sans-serif, sans-serif;
	text-decoration: none;
	margin: 0;
	padding:0;
	border: 0;
	width: 56px;
	height: 72px;
	background: #DFDFDF url('#{$imagePath}/voices/i/backtotop.png') no-repeat;
	z-index: 1000;
	text-indent: -999999px;
}

.ibm_cci-search-term-highlight{
	background: #dfdfdf;
}

.ibm_cci-colorClass1{
	background: #d8bebe;
}



.ibm_cci-colorClass2{
	background: #c8c8c8;
}

.ibm_cci-colorClass3{
	/*background: #f59999;*/
}

.ibm_cci--voices-divider{
	width: 100%;
	border:dotted #ccc; 
	border-width:1px 0 0 0; 
	height:0;
	line-height:0px;
	font-size:0;
	margin:0;
	padding:0 0 10px 0;
}

