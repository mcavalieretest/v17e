// li {outline:1px solid #00F}


/* IBM CONTENT BLOCK OVERIDES (BEGIN) */
#ibm-content-main { 
	background-color: #f8f8f8;
}


#ibm-pcon#{$ibm-pcon__modifier} {
	margin:0px;
	padding: 0 0 0 0;
	float:left;
}

#ibm-content#{$ibm-content__modifier}{
	background:none;
}
#ibm-content-body{
	// h1: 1.0em;
}
#ibm-content-main.ibm_cci-cb-overwrite{
	background-color:#fff;
}

#ibm-content-main{
//
	.ibm-band.ibm_cci__ibm-band-about {
		background: #fff;
		background: -moz-linear-gradient(top,  #f2f2f2 0%, #fff 0%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(0%,#fff));
		background: -webkit-linear-gradient(top,  #f2f2f2 0%,#fff 0%);
		background: -o-linear-gradient(top,  #f2f2f2 0%,#fff 0%);
		background: -ms-linear-gradient(top,  #f2f2f2 0%,#fff 0%);
		background: linear-gradient(to bottom,  #f2f2f2 0%,#fff 0%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f2f2f2', endColorstr='#fff',GradientType=0 );
		min-height:5px;	
		padding:0;
		box-shadow: inset 0 4px 8px -4px rgba(0,0,0,0.1);
		border-top: rgba(0,0,0,0.1) solid 1px;
	}
	.ibm-band#{$ibm-band__modifier} {
		background: #f8f8f8;
		background: -moz-linear-gradient(top,  #f2f2f2 0%, #f8f8f8 0%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(0%,#f8f8f8));
		background: -webkit-linear-gradient(top,  #f2f2f2 0%,#f8f8f8 0%);
		background: -o-linear-gradient(top,  #f2f2f2 0%,#f8f8f8 0%);
		background: -ms-linear-gradient(top,  #f2f2f2 0%,#f8f8f8 0%);
		background: linear-gradient(to bottom,  #f2f2f2 0%,#f8f8f8 0%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f2f2f2', endColorstr='#f8f8f8',GradientType=0 );
		min-height:5px;	
		padding:0;
		box-shadow: inset 0 4px 8px -4px rgba(0,0,0,0.1);
		border-top: rgba(0,0,0,0.1) solid 1px;
	}

	.ibm-band.ibm_cci__ibm-band-about{
		padding-bottom:20px;
	}
}


#{$ibm-widget-js}.ibm-columns.ibm_cci-gv__modifier > * a,
#{$ibm-widget-js}.ibm-columns.ibm_cci-lv__modifier > * a {
	text-decoration:none !important;
	color:#000;
}

#{$ibm-widget-js}.ibm-columns.ibm_cci-gv__modifier .ibm_cci-voicesLoader, 
#{$ibm-widget-js}.ibm-columns.ibm_cci-lv__modifier .ibm_cci-voicesLoader{
	width:100%;
	float:left;
	height:30px;
}


#{$ibm-widget-js} .ibm_cci-blocker {
	position: absolute;
	z-index: 9998;
	width: 250%;
	height: 100%;
	top: 0;
	background-color: #fff;
	margin-left:-500px;
	margin-top:-500px;
}

#{$ibm-widget-js} .ibm_cci-loader {
	position: absolute;
	z-index: 9999;
	width: 38px;
	height: 38px;
	opacity: .75;
	top: 150px;
	left: 50%;
	margin: -19px 0 0 -19px;
	background-image: url('#{$imagePath}/voices/i/loader.png');
}


/* GRID VIEW UNRESOLVED AND RESOLVED (BEGIN) ***/
.ibm-columns.ibm_cci-gv__modifier {
	margin:40px auto !important;
	// padding: 0 0 10px !important;
}

.ibm-columns.ibm_cci-gv__modifier .ibm-col-1-1 {
	float:left;
	width:300px;
	background-color: #fff;
	transition:width all 0.20s ease;
	-webkit-transition: all 0.20s ease; /* Safari */
	-moz-transition: all 0.20s ease;
	transition-duration:0.50s;
	-webkit-transition-duration: 0.50s;
	-moz-transition-duration: 0.50s;
	clear:none;
	outline:1px solid #ccc;
	margin-bottom:20px;
	min-height:170px;
	position:relative;
}
// GRID VIEW (END)


/* HOVER EFFECTS BEGIN */
@each $view in ibm_cci-lv__modifier, ibm_cci-gv__modifier {
	@if $view == ibm_cci-lv__modifier {
		#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:first-child:hover + .ibm-col-1-1
		{
			border-top:1px solid $hover-color;
		}
		#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:not(:nth-child(1)):hover
		{
			border-top:1px solid $hover-color;
		}
		#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:not(:nth-child(1)):hover + .ibm-col-1-1
		{
			border-top:1px solid $hover-color;
		}
		#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:last-child:hover
		{
			border-top:1px solid $hover-color;
			border-bottom:1px solid $hover-color;
		}

	}
	@else if $view == ibm_cci-gv__modifier{
		#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:hover
		{
			outline:1px solid $hover-color;
		}
	} 
	
	#{$ibm-widget-js}.ibm-columns.ibm_cci-lv__modifier .ibm-col-1-1:hover .ibm-col-6-2 .ibm_cci--lv-rc-sharing > ul,
	#{$ibm-widget-js}.ibm-columns.ibm_cci-gv__modifier .ibm-col-1-1:hover .ibm-col-6-2 .ibm_cci--lv-rc-sharing > ul,
	#{$ibm-widget-js}.ibm-columns.ibm_cci-lv__modifier .ibm-col-1-1:hover #{$ibm-listview-unro} .ibm_cci--unr-cb .ibm_cci--lv-rc-sharing > ul,
	#{$ibm-widget-js}.ibm-columns.ibm_cci-gv__modifier .ibm-col-1-1:hover #{$ibm-listview-unro} .ibm_cci--unr-cb .ibm_cci--lv-rc-sharing > ul,		
	#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:hover .ibm-col-6-4 .ibm_cci--rcb .ibm_cci-sib,
	#{$ibm-widget-js}.ibm-columns.#{$view} .ibm-col-1-1:hover .ibm-col-6-4 .ibm_cci--rcb .ibm_cci-gv-sib,
		{
			visibility:visible;
		}
}

	
#{$ibm-widget-js}.ibm-columns.ibm_cci-lv__modifier .ibm-col-1-1:hover .ibm_cci--rea-block ul li .ibm_cci--rea-b .ibm_cci--rea-b-e .ibm_cci--rea-b,
#{$ibm-widget-js}.ibm-columns.ibm_cci-gv__modifier .ibm-col-1-1:hover .ibm_cci--rea-block ul li .ibm_cci--rea-b .ibm_cci--rea-b-e .ibm_cci--rea-b
