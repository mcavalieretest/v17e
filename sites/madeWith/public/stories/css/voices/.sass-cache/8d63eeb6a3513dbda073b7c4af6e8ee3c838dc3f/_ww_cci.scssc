/* MODIFYERS FOR MOBILE (BEGIN) */
@media only screen and (min-width:0) and (max-width:568px){

	html {
		font-size:66%;
	}
	#ibm-top.ibm-landing-page #ibm-leadspace-head.ibm_cc__voices_ls {
		min-height:75px;
		#ibm-leadspace-body {
			width:300px;
			min-height:65px;
			padding:0;
		}	
	}

	#{$ibm-widget-js}.ibm_cci-blocker {
		width:400px;
	}

	.leadspace--defaults {
		margin:10px 0;
	}

	#ibm-leadspace-head.ibm_cc__voices_ls #ibm-leadspace-body #{$leadspace--logo}{
		width:126px;
		margin: 20px 0 10px 10px;
	}

	#ibm-leadspace-head #{$leadspace--logo} h1#{$leadspace__modifier} {
		background-position: -224px -38px;
		width:125px;
		height:28px;
		a {
			width:125px;
			line-height: 28px;
		}		
	}	

	#{$leadspace--search}, #{$leadspace--toggle}, .ibm_cci--sk.ibm_cci--sk__mt, #ibm-leadspace-body .ibm_cci--sr {
		display: none;
	}

	.ibm-band#{$ibm-band__modifier} {
		width:100%;
	}

	#ibm-top #ibm_cci-widget-js.ibm-columns.ibm_cci-gv__modifier .ibm-col-1-1 {
		width:300px;
	}

	#ibm-top .ibm-columns .ibm-col-1-1 {
		// margin:0 0 20px 10px;
		margin:0 0 20px 0;
	}

	#ibm-content.ibm_cci__ibm-content #ibm-content-body #ibm-content-main .ibm-band.ibm_cci__ibm-band {
		margin-bottom:20px;
	}

	#ibm-leadspace-head.ibm_cc__voices_ls #ibm-leadspace-body .ibm_cci--ls--logo p.ibm_cci-about, 
	#ibm-leadspace-head.ibm_cc__voices_ls #ibm-leadspace-body .ibm_cci--ls--logo p.ibm_cci-back
	{
		// top:30px;
		left:0px;
		// width:130px
				
	}

		//FOR ABOUT US PAGE
		//
		#ibm-content #{$ibm-widget-about}.ibm-columns .ibm-col-6-2.ibm_cci-rr{
			margin-top:20px;
		}	
		#ibm-content #{$ibm-widget-about}.ibm-columns .ibm-col-6-4.ibm_cci_acc{
			position:relative;
			top:-20px;
		}

		.ibm-legal-text, .ibm-legal {
			width:300px;
		}						}


/* MODIFYERS FOR TABLETS (BEGIN) */
@media only screen and (min-width:641px) and (max-width:800px) {
	
	html {
		font-size:66%;
	}

	#ibm-top.ibm-landing-page #ibm-leadspace-head.ibm_cc__voices_ls {
		min-height:100px;
		
		#ibm-leadspace-body, .ibm_cci--sk.ibm_cci--sk__mt {
			width:730px;
		}

		#ibm-leadspace-body{	
			min-height:100px;
			padding:0;
		}

	}

	.leadspace--defaults {
		margin:20px 0;
	}

	#ibm-leadspace-head.ibm_cc__voices_ls #ibm-leadspace-body #{$leadspace--logo}{
		width:250px;
		margin: 40px 0;
	}

	#ibm-leadspace-head #{$leadspace--logo} h1#{$leadspace__modifier} {
		background-position: -224px 0;
		width:167px;
		height:37px;
		a {
			width:167px;
			line-height: 37px;
		}
	}
	#{$leadspace--toggle} {
		width: 86px; 
	}

	#{$leadspace--search} {
		width: 360px;
	}

	#{$leadspace--search} form input.ibm-cc-search--field {
		width:285px; 
		margin-top:5px;
	}

	// #ibm-leadspace-head.ibm_cc__voices_ls #ibm-leadspace-body .ibm_cci--sr {
