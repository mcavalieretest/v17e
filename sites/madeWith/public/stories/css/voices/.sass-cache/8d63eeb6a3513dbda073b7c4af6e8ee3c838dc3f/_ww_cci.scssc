		.ibm-columns.ibm_cci-accounts{
			margin-top:20px;

			.ibm-col-6-2.ibm_cci-ba, .ibm-col-6-2.ibm_cci-ia{
				width:310px;
			}

			.ibm-col-6-2.ibm_cci-ia .ibm-columns{
				.ibm-col-6-1 {
					width: 145px;
				}
			}
		}
	}	

	#ibm-content #{$ibm-widget-about}.ibm-columns .ibm-col-6-2.ibm_cci-rr{
		width:640px;
		margin-top:25px;
		img {
					// width:274px;
					float:left;
				}

				p.ibm_cci-rea-desc {
					float:left;
